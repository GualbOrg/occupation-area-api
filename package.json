{
  "name": "aws-nestjs-starter",
  "version": "0.0.1",
  "description": "A starter project that makes creating a deployable AWS Serverless project extremely easy",
  "author": "hardyscc",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:online": "cross-env IS_DDB_LOCAL= nest start",
    "deploy": "serverless  deploy --stage dev --region us-east-1 --verbose --aws-profile factory-worker",
    "package": "sls package",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "test:cov": "jest --verbose --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ddb:install": "sls dynamodb install",
    "ddb:start": "sls dynamodb start",
    "sls:offline": "cross-env NO_COLOR= sls offline",
    "sls:online": "cross-env NO_COLOR= cross-env IS_DDB_LOCAL= sls offline",
    "genres": "ts-node genres.ts -- \"src/**/*.schema.ts\" \"resource/dynamodb.yml\""
  },
  "dependencies": {
    "@apollo/subgraph": "^2.2.1",
    "@graphql-tools/merge": "8.3.12",
    "@nestjs/apollo": "10.1.6",
    "@nestjs/common": "9.2.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/graphql": "10.1.6",
    "@nestjs/platform-express": "9.2.0",
    "@prisma/client": "^4.6.1",
    "@vendia/serverless-express": "4.10.1",
    "apollo-server-express": "3.11.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dataloader": "2.0.0",
    "graphql": "16.6.0",
    "husky": "8.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "uuid": "9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.0",
    "@types/aws-lambda": "8.10.108",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.18.3",
    "@types/passport-jwt": "^3.0.7",
    "@types/serverless": "3.12.9",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "change-case": "4.1.2",
    "cross-env": "7.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "29.3.1",
    "js-yaml": "4.1.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prisma": "^4.6.1",
    "serverless": "3.24.1",
    "serverless-add-api-key": "^4.2.1",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "11.3.0",
    "serverless-plugin-typescript": "2.1.4",
    "serverless-webpack": "^5.10.0",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.9.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}